domain Risk

export const SEVERITY = {
    LOW: 'Low',
    MEDIUM: 'Medium',
    HIGH: 'High'
}

export const LIKELIHOOD = {
    LOW: 'Low',
    MEDIUM: 'Medium',
    HIGH: 'High'
}

export model Risk {
    description: text "A description of the risk"
